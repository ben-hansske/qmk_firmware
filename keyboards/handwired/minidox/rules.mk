MCU = atmega32u4
F_CPU = 16000000
ARCH = AVR8
F_USB= $(F_CPU)
OPT_DEFS += -DINTERRUPT_CONTROL_ENDPOINT
BOOTLOADER = atmel-dfu
BOOTMAGIC_ENABLE = lite
MOUSEKEY_ENABLE = yes
EXTRAKEY_ENABLE = yes
CONSOLE_ENABLE = yes
COMMAND_ENABLE = no
NKRO_ENABLE = yes
AUDIO_ENABLE = no
RGBLIGHT_ENABLE = yes
LEADER_ENABLE = no
MIDI_ENABLE = no
UNICODE_ENABLE = no
BLUETOOTH = no
SPLIT_KEYBOARD = yes
